{
  "name": "chess-player-api",
  "version": "1.0.0",
  "description": "AWS Lambda API with DynamoDB",
  "main": "handler.js",
  "dependencies": {
    "aws-sdk": "^2.751.0",
    "remark-cli": "^8.0.1",
    "remark-lint": "^7.0.1",
    "remark-preset-lint-recommended": "^4.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.62",
    "@types/aws-sdk": "^2.7.0",
    "@types/jest": "^26.0.13",
    "jest": "^26.4.2",
    "serverless-offline": "^6.7.0",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-dynamodb-local": "^0.2.39",
    "ts-jest": "^26.3.0",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "test": "jest",
    "lint-md": "remark ."
  },
  "keywords": [
    "chess",
    "api",
    "chess players"
  ],
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ]
  },
  "moduleFileExtensions": [
    "js",
    "ts"
  ],
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "author": "Matt D Webb",
  "license": "MIT"
}
